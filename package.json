{
  "name": "Explainer-timer",
  "version": "1.1.0",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.21.2",
    "bluebird": "^3.7.2",
    "bootstrap": "^5.3.2",
    "electron-is-dev": "^1.1.0",
    "knex": "^3.1.0",
    "node-cron": "^3.0.3",
    "node-zklib": "^1.3.0",
    "python-struct": "^1.1.3",
    "qr-zklib": "^1.0.3",
    "react": "^16.12.0",
    "react-data-table-component": "^7.6.2",
    "react-dom": "^16.12.0",
    "react-flip-move": "^3.0.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.3.1",
    "reactstrap": "^9.2.2",
    "sqlite3": "^4.1.1",
    "styled-components": "^6.1.8",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder --win -c.extraMetadata.main=build/electron.js",
    "electron-start": "electron ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^7.1.11",
    "electron-builder": "^22.3.2"
  },
  "author": {
    "name": "Inayat Ullah",
    "email": "inayatUllahkks@gmail.com",
    "url": "https://explainerkhan.com/"
  },
  "build": {
    "appId": "com.explainerkhan.time-attendance",
    "productName": "Explainer Timer",
    "copyright": "Copyright Â© 2024 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "node_zklib/**/*",
      "backend/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
